<p id="notice"><%= notice %></p>

<h1>Listing User Place Category Matches</h1>

<table>
  <thead>
    <tr>
      <th>Match</th>
      <th>Upc</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_place_category_matches.each do |user_place_category_match| %>
      <tr>
        <td><%= user_place_category_match.match_id %></td>
        <td><%= user_place_category_match.upc_id %></td>
        <td><%= link_to 'Show', user_place_category_match %></td>
        <td><%= link_to 'Edit', edit_user_place_category_match_path(user_place_category_match) %></td>
        <td><%= link_to 'Destroy', user_place_category_match, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User place category match', new_user_place_category_match_path %>
