<p id="notice"><%= notice %></p>

<h1>Listing User Places</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Place</th>
      <th>Desc</th>
      <th>Rating</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_places.each do |user_place| %>
      <tr>
        <td><%= user_place.user_id %></td>
        <td><%= user_place.place_id %></td>
        <td><%= user_place.desc %></td>
        <td><%= user_place.rating %></td>
        <td><%= link_to 'Show', user_place %></td>
        <td><%= link_to 'Edit', edit_user_place_path(user_place) %></td>
        <td><%= link_to 'Destroy', user_place, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User place', new_user_place_path %>
